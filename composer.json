{
    "name": "turbo/lambostarter",
    "description": "TurboFrame - Fast PHP Framework",
    "type": "framework",
    "keywords": [
        "framework",
        "php",
        "turboframe",
        "fast",
        "mvc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "TurboFrame Community",
            "email": "community@turboframe.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.5",
        "ext-pdo": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "TurboFrame\\": "src/",
            "App\\": "application/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "serve": "php lambo serve",
        "nitrous": "php lambo nitrous"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}